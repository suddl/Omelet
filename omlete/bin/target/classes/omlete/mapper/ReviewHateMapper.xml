<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="omlete.mapper.ReviewHateMapper">
	<insert id="insertReviewHate">
		insert into review_hate values(#{reviewHateNo}, #{reviewHateMember}, #{reviewHateApplyNo})
	</insert>
	
	<delete id="deleteReviewHate">
		delete from review_Hate where review_hate_no = #{reviewHateNo}
	</delete>
</mapper>