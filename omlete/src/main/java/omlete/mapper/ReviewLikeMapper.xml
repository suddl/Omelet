<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="omlete.mapper.ReviewLikeMapper">
	<insert id="insertReviewLike">
		insert into review_like values(#{reviewLikeNo}, #{reviewLikeMember}, #{reviewLikeApplyNo})
	</insert>
	
	<delete id="deleteReviewLike">
		delete from review_like where review_like_no = #{reviewLikeNo}
	</delete>
</mapper>